
<html><head><meta charset="UTF-8">
<link rel="icon" type="image/x-icon" href="{{asset('favicon.ico')}}" />
<link rel="shortcut" type="image/x-icon" href="{{asset('favicon.ico')}}" />
{% block title %}
  <title>{{ 'bienvenue.site' | trans }}</title>
    <link rel="prefetch" href="{{ asset('common/sky11ss.jpg') }}">
{% endblock %}
{% block stylesheets %}
   <link href="{{ asset('css/main.css') }}" rel="stylesheet" />
   <link href="{{ asset('css/header/header.css') }}" rel="stylesheet" />
   <link href="{{ asset('css/footer/footer.css') }}" rel="stylesheet" />
{% endblock %}

{% block js %}

   <script src="{{asset('js/main.js')}}"></script>
{% endblock %}


</head>
<body  >

<div id="page" >
    <div class='header' id='header' >
        {% block myheader %}
        {% include 'header.html.twig' %}
       
        {% endblock %}
    </div>
    
    <div class='mypage'>    
        <div class='mypageinner'>   
            {% block mypage %}
            <div id="left" >
                {% block left %}
                <div><p>left </p></div>
                {% endblock %}
            </div>
            <div id="middle" >
                {% block middle %}
                <div><p> middle</p></div>
                {% endblock %}
            </div>
            <div id="right" >
                {% block right %}
                <div> <p>right </p></div>
                {% endblock %}
            </div>
        </div>
    </div>
    
    {% endblock %}
    
    
    <div id="footer">
        {% block myfooter %}
            {% include 'footer.html.twig' %}
        {% endblock %}  
    </div>
</div>
 {% block quill %}
 {% endblock %}
</body>      

</html>
