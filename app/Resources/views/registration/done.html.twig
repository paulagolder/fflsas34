{# templates/registration/done.html.twig #}


{% import 'macros.twig' as m %}

{% extends 'base.html.twig' %}

{% block stylesheets %}
      {{ parent() }}
   <link href="{{ asset('/css/registration/regack.css') }}" rel="stylesheet" />
{% endblock %}



{% block middle %}
<div id="regack" >
<div class="label" >{{'welcome.to.our.site' | trans}}</div>
<div class="heading">
<div class="label" >{{'username' | trans }}:</div>
<div class="title">{{username}}</div>
</div>
<div class="heading">
<div class="label" >{{'email' | trans }}:</div>
<div class="title">{{email}}</div>
</div>
<div class="message">
{{ message }}
</div>

</div>

{% endblock %}
