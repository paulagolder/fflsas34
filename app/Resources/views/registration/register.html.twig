{# templates/registration/register.html.twig #}


{% import 'macros.twig' as m %}

{% extends 'base.html.twig' %}

{% block stylesheets %}
      {{ parent() }}
   <link href="{{ asset('/css/person/showone.css') }}" rel="stylesheet" />
{% endblock %}



{% block middle %}


{{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
