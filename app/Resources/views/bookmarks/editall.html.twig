{# templates/location/editone.html.twig #}

{% extends 'admin.html.twig' %}

{% block stylesheets %}
      {{ parent() }}
   <link href="{{ asset('bookmark_css/bookmarks.css') }}" rel="stylesheet" />
 
{% endblock %}
{% block js %}
 {{ parent() }}
{% endblock %}




{% block middle %}


   <div id="bookmarks" >
 
     <div class="heading">
       <div class="label">{% trans %}bookmarks{% endtrans %}</div> 
       </div>
     {% for key1, blist in blists  %}
        <div class="bookmark{{key1}}"> {{key1}}</div>
       {% for key in blist | keys  %} 
    
      <div class="bookmark{{key1}}" >
        <button onclick="deletebookmark('{{key1}}',{{blist[key]['id']}});">{{blist[key]['label'] }}</button>
      </div>

      {%endfor%}
     {% endfor %}
    </div>
{% endblock %}
