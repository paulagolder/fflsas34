{# templates/contact/contact.html.twig #}

{% extends 'admin.html.twig' %}

{% block stylesheets %}
      {{ parent() }}
   <link href="{{ asset('css/image_css/showall.css') }}" rel="stylesheet" />
{% endblock %}


{% block middle %}
  
   {% if message %}
     <h1> {{message}}</h1>
   {% endif %}
  <form  action="/admin/image/search">
   <span> {{ 'entrez.chercher' | trans }}</span>
   <input type="text" name="searchfield">
   <input type="submit" value="{{ 'chercher' | trans }}" >
   </form>
 
   {% if images is defined %}
     <h1>{{heading}}</h1> 
     <nav>
       <ul>
         {% for image in images  %}
            <li><a href="{{ image.link}}" >{{ image.path}}</a> </li>
         {% endfor %}
      </ul>
     </nav>
  {% endif %}
{% endblock %}



