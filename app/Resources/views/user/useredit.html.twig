{# templates/user/edit.html.twig #}


{% extends 'admin.html.twig' %}

{% block stylesheets %}
      {{ parent() }}
   
{% endblock %}


{% block middle %}
<div id="middle" >
<div id="usereditform"  >
<div class="heading">
 <div class="label" > {{'edit.user' |trans }}</div>

 <div class="returnbutton"> <a href = {{returnlink}} >{{'retour' | trans }} </a> </div>
 </div>
<div  class="form" >
{{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.plainPassword) }}
    {{ form_row(form.rolestr) }}
    {{ form_row(form.email) }}

    <button type="submit">Register!</button>
{{ form_end(form) }}

</div>
</div>
</div>
{% endblock %}
