{# templates/registration/register.html.twig #}


{% import 'macros.twig' as m %}

{% extends 'admin.html.twig' %}

{% block stylesheets %}
      {{ parent() }}
   <link href="{{ asset('css/user_css/edit.css') }}" rel="stylesheet" />
{% endblock %}


{% block middle %}

<div id="fuser"  >
 <div class="formheading" > {%trans%}edit.person{% endtrans%}</div>

<div  class="userform" >
{{ form_start(form) }}

    {{ form_row(form.username) }}
    {{ form_row(form.plainPassword) }}
    {{ form_row(form.Rolestr) }}
    {{ form_row(form.email) }}

    <button type="submit">Register!</button>
{{ form_end(form) }}


<div> valid:{{isvalid}}</div>

 <div class="returnbutton"> <a href = {{returnlink}} >{% trans %} retour {% endtrans%} </a> </div>
</div>
{% endblock %}
